<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <RecipeDef Name="CookDonkPocket">
    <defName>CookDonkPocket</defName>
    <recipeUsers>
      <li>ElectricStove</li>
      <li>FueledStove</li>
    </recipeUsers>
    <label>cook donk pocket</label>
    <description>Cooks a Donk Pocket.</description>
    <jobString>Cooking Donk Pocket.</jobString>
    <workSpeedStat>CookSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_CookMeal</soundWorking>
    <allowMixingIngredients>true</allowMixingIngredients>
    <ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>MeatRaw</li>
            <li>AnimalProductRaw</li>
          </categories>
        </filter>
        <count>0.5</count>
      </li>
    </ingredients>
    <products>
      <DonkPocket>1</DonkPocket>
    </products>
    <fixedIngredientFilter>
      <categories>
        <li>FoodRaw</li>
      </categories>
      <specialFiltersToDisallow>
        <li>AllowPlantFood</li>
      </specialFiltersToDisallow>
    </fixedIngredientFilter>
    <defaultIngredientFilter>
      <categories>
        <li>MeatRaw</li>
      </categories>
      <disallowedCategories>
        <li>EggsFertilized</li>
      </disallowedCategories>
      <disallowedThingDefs>
        <li>Meat_Human</li>
      </disallowedThingDefs>
    </defaultIngredientFilter>
    <workSkill>Cooking</workSkill>
  </RecipeDef>
</Defs>